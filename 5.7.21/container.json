{
	"project": "wplib",
	"name": "mysql",
	"version": "5.7.21",
	"majorversion": "5.7",
	"latest": "1",
	"class": "database",
	"network": "--network wplibbox",
	"ports": "-p 3306:3306",
	"volumes": "-v /vagrant/sql:/tmp/sql -v /vagrant/provision:/docker-entrypoint-initdb.d -v mysql_data:/var/lib/mysql",
	"restart": "--restart no",
	"args": "-e MYSQL_ALLOW_EMPTY_PASSWORD=yes -e MYSQL_DATABASE=wordpress -e MYSQL_USER=wordpress -e MYSQL_PASSWORD=wordpress",
	"env": "MYSQL_VERSION"
}

